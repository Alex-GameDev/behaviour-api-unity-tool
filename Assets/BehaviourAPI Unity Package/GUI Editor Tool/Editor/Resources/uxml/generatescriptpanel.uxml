            <ui:TextField picking-mode="Ignore" value="newbehaviourgraph" text="assets/scripts/" name="gsp-path-field" max-length="-1" label="path" style="font-size: 16px; margin-top: 5px; margin-left: 3px; margin-right: 3px; margin-bottom: 5px;" />
